                  },
                       
     "subnet": {
                     
           "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_vnet1_name')), '/subnets/subn1')]"
                   
         }
               
         }
                   
 }
               
 ],
              
  "dnsSettings": {
       
             "dnsServers": []
      
          },
           
     "enableIPForwarding": false,
         
